node {
  environment {
      DOCKERHUB_USER = credentials('DOCKERHUB_USER')
      DOCKERHUB_PASS = credentials('DOCKERHUB_PASS')
  }
  stage('pre-build') {
    checkout scm
  }
  stage('build'){
    sh 'docker build -t muhammadosama/sqlite-project:jenkins-ec2 .'
  }
  stage('push'){
    sh 'docker images'
  }
}
